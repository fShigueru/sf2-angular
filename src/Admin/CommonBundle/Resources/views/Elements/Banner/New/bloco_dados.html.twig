                            <span id="inputSuccess2Status2" class="sr-only">(success)</span>
                        </div>
                    </div>
                    <div class="form-group {{ form.dtExpiracao.vars.required ? 'item' : '' }}">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="{{ form.dtExpiracao.vars.id }}">{{ form_label(form.dtExpiracao) }}
                            {{ form.dtExpiracao.vars.required ? '<span class="required">*</span>' : '' }}
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            {{ form_row(form.dtExpiracao, { 'label': false }) }}
                            <span class="fa fa-calendar-o form-control-feedback left" aria-hidden="true"></span>
                            <span id="inputSuccess2Status2" class="sr-only">(success)</span>
                        </div>
                    </div>
                    <div class="form-group {{ form.status.vars.required ? 'item' : '' }}">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="{{ form.status.vars.id }}">{{ form.status.vars.label|transchoice(0,{}, 'messagesCommonBundle') }}
                            {{ form.status.vars.required ? '<span class="required">*</span>' : '' }}
                        </label>
                        <div class="col-md-7 col-sm-7 col-xs-12">
                            {{ form_row(form.status, { 'label': false }) }}
                        </div>
                        <br clear="all" />
                    </div>
                </div>
            </div>
        </div>
    </div>
