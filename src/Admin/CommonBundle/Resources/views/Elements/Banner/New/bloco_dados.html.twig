            <li class="dropdown"></li>
        </ul>
        <div class="clearfix"></div>
    </div>
    <div class="x_content">
        <div class="row">
            <div class="form-horizontal form-label-left">
                <div class="col-md-12 col-sm-12 col-xs-12 form-group">

                    <div class="form-group {{ form.dtPublicacao.vars.required ? 'item' : '' }}">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="{{ form.dtPublicacao.vars.id }}">{{ form_label(form.dtPublicacao) }}
                            {{ form.dtPublicacao.vars.required ? '<span class="required">*</span>' : '' }}
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            {{ form_row(form.dtPublicacao, { 'label': false }) }}
