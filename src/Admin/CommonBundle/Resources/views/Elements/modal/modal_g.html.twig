<div class="modal fade {{ modal }}" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">Ã—</span>
                </button>
                <h3 class="modal-title" id="myModalLabel">
                    <i class="fa {{ iconTitle }}"></i>
                    <small>{{ title|transchoice(0,{}, bundle) }}</small>
                </h3>
            </div>
            <div class="modal-body">
                <p>{{ descricao|transchoice(0,{}, bundle) }}</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">
                    {{ 'title.fechar'|transchoice(0,{}, 'messagesCommonBundle') }}
                </button>
            </div>
        </div>
    </div>
</div>