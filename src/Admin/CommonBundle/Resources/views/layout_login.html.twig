<!DOCTYPE html>
<html ng-app="app">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}{{ configuracao.valor("titulo.site.completo") }}{% endblock %}</title>
    {% block stylesheets %}
        {% stylesheets
        '@bootstrap_css'
        '@AdminCommonBundle/Resources/public/fonts/css/font-awesome.min.css'
        '@AdminCommonBundle/Resources/public/css/animate.min.css'
        '@AdminCommonBundle/Resources/public/css/custom.css'
        '@AdminCommonBundle/Resources/public/css/icheck/flat/green.css'
        '@AdminCommonBundle/Resources/public/css/switchery/switchery.min.css'
        combine=true
        filter='uglifycss'
        filter='cssrewrite'
        output='css/cms/login.css'
        %}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body class="nav-md" style="background:#F7F7F7;">
{% block body %}
    {% block fos_user_content %}{% endblock %}
{% endblock %}
{% block javascripts %}
    {% javascripts
    '@jquery'
    '@bootstrap'
    '@angularjs'
    '@AdminCommonBundle/Resources/public/js/tags/jquery.tagsinput.min.js'
    '@AdminCommonBundle/Resources/public/js/switchery/switchery.min.js'
    '@AdminCommonBundle/Resources/public/js/editor/bootstrap-wysiwyg.js'
    '@AdminCommonBundle/Resources/public/js/validator/validator.js'
    '@AdminCommonBundle/Resources/public/js/custom.js'
    combine=true
    filter='uglifyjs2'
    output='js/cms/login.js'
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <script>
        angular.module("app", []).config(function($interpolateProvider){
            $interpolateProvider.startSymbol("{[{").endSymbol("}]}");
        });
    </script>
    <!--[if lt IE 9]>
    <!--<script src="../assets/js/ie8-responsive-file-warning.js"></script>-->
    <![endif]-->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

{% endblock %}
</body>
</html>