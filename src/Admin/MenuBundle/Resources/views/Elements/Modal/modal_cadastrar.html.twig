<div class="modal fade {{ modal }}" tabindex="-1" role="dialog" aria-hidden="true" ng-controller="NewMenuCtrl">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">Ã—</span>
                </button>
                <h3 class="modal-title" id="myModalLabel">
                    <i class="fa {{ iconTitle }}"></i>
                    <small>{{ title|transchoice(0,{}, bundle) }}</small>
                </h3>
            </div>
            <div class="modal-body">
                <p>{{ descricao|transchoice(0,{}, bundle) }}</p>
                <div class="x_content">
                    <div class="row">
                        <div class="form-horizontal form-label-left">
                            <div class="col-md-12 col-sm-12 col-xs-12 form-group">
                                <div class="form-group {{ form.nome.vars.required ? 'item' : '' }}">
                                    <label class="control-label col-md-2 col-sm-2 col-xs-12" for="{{ form.nome.vars.id }}">
                                        {{ form.nome.vars.label|transchoice(0,{}, 'messagesCommonBundle') }}
                                        {{ form.nome.vars.required ? '<span class="required">*</span>' : '' }}
                                    </label>
                                    <div class="col-md-9 col-sm-9 col-xs-12">
                                        <span class="fa fa-align-left form-control-feedback left" aria-hidden="true"></span>
                                        {{ form_row(form.nome, { 'label': false }) }}
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-horizontal form-label-left">
                            <div class="col-md-12 col-sm-12 col-xs-12 form-group">
                                <div class="form-group {{ form.url.vars.required ? 'item' : '' }}">
                                    <label class="control-label col-md-2 col-sm-2 col-xs-12" for="{{ form.url.vars.id }}">
                                        {{ form.url.vars.label|transchoice(0,{}, 'messagesCommonBundle') }}
                                        {{ form.url.vars.required ? '<span class="required">*</span>' : '' }}
                                    </label>
                                    <div class="col-md-9 col-sm-9 col-xs-12">
                                        <span class="fa fa-align-left form-control-feedback left" aria-hidden="true"></span>
                                        {{ form_row(form.url, { 'label': false }) }}
                                        <span class="label label-info label-info-admin">
                                            {{ 'title.formato.url'|transchoice(0,{}, 'messagesCommonBundle') }}
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="clearfix"></div>
            <div class="modal-footer">
                {{ form_widget(form.button) }}
                <div class="loader"></div>
                <button type="button" class="btn btn-default fechar" data-dismiss="modal">
                    {{ 'title.fechar'|transchoice(0,{}, 'messagesCommonBundle') }}
                </button>
            </div>
        </div>
    </div>
</div>