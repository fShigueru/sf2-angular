<div class="">
    <div class="page-title">
        <div class="title_left">
            <h1><small>{{ 'title.menu'|transchoice(0,{}, 'messagesMenuBundle') }}</small></h1>
        </div>
        <div class="title_right">
            <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
                <div class="input-group">
                </div>
            </div>
        </div>
    </div>
    <div class="clearfix"></div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_title">
                    <h2>{{ 'title.cadastrar.item.menu'|transchoice(0,{}, 'messagesMenuBundle') }}</h2>
                    <i class="fa fa-question red padding-left-7 padding-top-8 cursor-pointer" data-toggle="modal" data-target=".modal-duvida-url-descricao"></i>
                    <ul class="nav navbar-right panel_toolbox">
                        <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                        </li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                            <ul class="dropdown-menu" role="menu">
                                <li><a href="#">Settings 1</a>
                                </li>
                                <li><a href="#">Settings 2</a>
                                </li>
                            </ul>
                        </li>
                        <li><a href="#"><i class="fa fa-close"></i></a>
                        </li>
                    </ul>
                    <div class="clearfix"></div>
                </div>
                <div class="x_content">
                    <div class="form-group">
                        <div class="col-md-9 col-sm-9 col-xs-12">
                            <button class="btn btn-dark" data-toggle="modal" data-target=".modal-menu-new">
                                {{ 'title.novo'|transchoice(0,{}, 'messagesMenuBundle') }}
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
{% set modalMenuNew = {
'title': 'title.novo',
'descricao': 'title.novo.descricao',
'modal': 'modal-menu-new',
'iconTitle':'fa-info-circle',
'bundle':'messagesMenuBundle'} %}
{{ include('@AdminMenu/Elements/Modal/modal_cadastrar.html.twig', modalMenuNew) }}

{% set modalMenuNewInfo = {
'title': 'title.cadastrar.item.menu',
'descricao': 'title.cadastrar.item.menu.descricao',
'modal': 'modal-duvida-url-descricao',
'iconTitle':'fa-info-circle',
'bundle':'messagesMenuBundle'} %}
{{ include('@AdminCommon/Elements/modal/modal_g.html.twig', modalMenuNewInfo) }}